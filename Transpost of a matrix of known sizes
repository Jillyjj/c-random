int main (){
int mat[][3]={
        {1,2,3}
        ,{4,5,6}
    };
int mat_2 [3][2];
for(int i=0; i<2; i++){
    for(int j=0; j<3; j++){
        mat_2[j][i]=mat[i][j];
    }
}
    for(int i=0; i<3; i++){
        for(int j=0; j<2; j++){
            printf("%d " , mat_2[i][j]);
        }
        printf("\n");
    }
    return 0;
}
